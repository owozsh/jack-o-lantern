      "settings": {
        "foreground": "#b97900",
        "fontStyle": "italic"
      }
    },
    // YELLOW_UNDERLINE
    {
      "scope": ["entity.name.type.class", "entity.name.type"],
      "settings": {
        "foreground": "#b97900",
        "fontStyle": "underline"
      }
    },
    // BLUE
    {
      "scope": ["constant.numeric.css", "constant.numeric", "constant"],
      "settings": {
        "foreground": "#a05373",
        "fontStyle": ""
      }
    },
    // BLUE_ITALIC
    {
      "scope": [
        "constant.language",
        "punctuation.definition.entity.html",
        "constant.character.entity.named.emsp.html"
      ],
      "settings": {
        "foreground": "#a05373",
        "fontStyle": "italic"
      }
    },
    // CYAN
